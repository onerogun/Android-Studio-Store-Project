        tools:layout="@layout/musical_instruments_fragment" />

    <fragment
        android:id="@+id/personal_care"
        android:name="com.og.prj.ui.personalcare.PersonalCareFragment"
        android:label="@string/personal_care_section"
        tools:layout="@layout/personal_care_fragment" />

    <fragment
        android:id="@+id/contact_us"
        android:name="com.og.prj.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/content_contact_us" />

    <fragment
        android:id="@+id/show_map"
        android:name="com.og.prj.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/activity_store_location"/>
