<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/cell_phones">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.og.prj.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/cell_phones"
        android:name="com.og.prj.ui.cellphone.CellPhoneFragment"
        android:label="@string/cell_phones_section"
        tools:layout="@layout/cell_phone_fragment" />

    <fragment
        android:id="@+id/computers"
        android:name="com.og.prj.ui.computer.ComputerFragment"
        android:label="@string/computers_section"
        tools:layout="@layout/computer_fragment" />

    <fragment
        android:id="@+id/musical_instruments"
        android:name="com.og.prj.ui.musicalinstruments.MusicalInstrumentsFragment"
